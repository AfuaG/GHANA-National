              <boolProp name="resetInterpreter">true</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">import java.util.regex.Pattern;
import java.util.regex.Matcher;

response = new String(data);
pattern = Pattern.compile(&quot;&lt;input(?!&lt;input).*?/&gt;&quot;, Pattern.DOTALL);
matcher= pattern.matcher(response);

staffIds = vars.get(&quot;staffIds&quot;);
staffIds = staffIds != null ? staffIds : &quot;&quot;;

while(matcher.find())
{
    inputTag = matcher.group(0);
    if(inputTag.contains(&quot;staffId&quot;)){
        substringFromValueAttribute = inputTag.substring(inputTag.indexOf(&quot;value=&quot;));
        staffIdWithQuotes = substringFromValueAttribute.substring(6, substringFromValueAttribute.indexOf(&quot; &quot;));       
        vars.put(&quot;staffIds&quot;, (staffIds + &quot; &quot; + staffIdWithQuotes.substring(1, staffIdWithQuotes.length() - 1)).trim());
        break;
    }
}

</stringProp>
