        <xf:bind id="deliveryDateConfirmed" nodeset="/ANCRegistration/deliveryDateConfirmed" required="true()"
                 type="xsd:string"/>
        <xf:bind constraint=". &gt;= 60 and . &lt;= 200" id="height"
