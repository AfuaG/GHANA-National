                 required="true()" type="xsd:decimal"/>
        <xf:bind constraint=". &gt;= 1 and . &lt;= 30" id="gravida"
                 message="Value out of range: must be between 1 and 30" nodeset="/ANCRegistration/gravida"
                 required="true()" type="xsd:int"/>
