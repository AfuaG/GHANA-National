        <xf:bind id="facilityId" nodeset="/ANCRegistration/facilityId" required="true()" type="xsd:int"
                 constraint=". &lt; 2147483647" message="Number too large. Keep under 2147483647"/>
        <xf:bind id="regDateToday" nodeset="/ANCRegistration/regDateToday" required="true()" type="xsd:string"/>
        <xf:bind constraint=". &lt;= 'today()'" id="date" message="Invalid date: date entered is in the future"
                 relevant="/ANCRegistration/regDateToday = 'IN_PAST' or /ANCRegistration/regDateToday = 'IN_PAST_IN_OTHER_FACILITY'"
                 visible="false()" action="show" nodeset="/ANCRegistration/date" required="true()" type="xsd:date"/>
